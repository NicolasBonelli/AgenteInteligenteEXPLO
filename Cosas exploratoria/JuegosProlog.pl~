todos_los_juegos(Juegos) :-
    findall(Juego, videojuego(Juego), Juegos).
videojuegos_por_precio(Precio, Videojuegos) :-
    findall(Videojuego, precio(Videojuego, Precio), Videojuegos).
videojuegos_por_precio_mayor(Precio,Videojuegos) :-
    findall(Videojuego,( precio(Videojuego, P),P>Precio), Videojuegos).
videojuegos_por_precio_menor(Precio,Videojuegos) :-
    findall(Videojuego,( precio(Videojuego, P),P<Precio), Videojuegos).
videojuegos_por_categoria(Categoria, Videojuegos) :-
    findall(Videojuego, categoria(Videojuego, Categoria), Videojuegos).
videojuegos_por_rating(Rating,Videojuegos) :-
    findall(Videojuego, rating(Videojuego, Rating), Videojuegos).
videojuegos_por_rating_mayor(Rating,Videojuegos) :-
    findall(Videojuego,( rating(Videojuego, R),R>Rating), Videojuegos).
videojuegos_por_rating_menor(Rating,Videojuegos) :-
    findall(Videojuego,( rating(Videojuego, R),R<Rating), Videojuegos).
videojuegos_por_rating_mayor_categoria(Rating, Categoria, Videojuegos) :-
    findall(Videojuego,(rating(Videojuego, R), R > Rating, categoria(Videojuego, Categoria)),Videojuegos).
videojuegos_por_rating_menor_categoria(Rating, Categoria, Videojuegos) :-
    findall(Videojuego,(rating(Videojuego, R), Rating>R, categoria(Videojuego, Categoria)),Videojuegos).
datos_juego(Juego, Precio, Categoria, Rating) :- videojuego(Juego), precio(Juego, Precio),categoria(Juego, Categoria),rating(Juego, Rating).
caracteristicas_videojuego(Juego,Categoria,Precio,Rating,Conexion,Productora):-videojuego(Juego),categoria(Juego, Categoria),rating(Juego, Rating),precio(Juego,Precio),conexion(Juego,Conexion),productora(Juego,Productora) .
buscar_juego(Categoria, Precio, Rating, Conexion, Productora, Juego) :-
    videojuego(Juego),
    categoria(Juego, Categoria),
    precio(Juego, Precio),
    rating(Juego, Rating),
    conexion(Juego, Conexion),
    productora(Juego, Productora).




